    <insert id="save" parameterType="com.bootdo.futures.domain.FuturesDO" useGeneratedKeys="true" keyProperty="id">
		insert into tb_futures
		(
			`trancode`, 
			`time`, 
			`opening`, 
			`highest`, 
			`lowest`, 
			`closing`,
        `volume`,
        <if test="maMa1 != null">`ma_ma1`,</if>
        <if test="maMa2 != null">`ma_ma2`,</if>
        <if test="maMa3 != null">`ma_ma3`,</if>
        <if test="maMa4 != null">`ma_ma4`,</if>
        <if test="maMa5 != null">`ma_ma5`,</if>
        <if test="maMa6 != null">`ma_ma6`,</if>
        <if test="maMa7 != null">`ma_ma7`,</if>
        <if test="maMa8 != null">`ma_ma8`,</if>
        `vol_volume`
        <if test="volMavol1 != null">,`vol_mavol1`,</if>
        <if test="volMavol2 != null">`vol_mavol2`,</if>
        <if test="macdDif != null">`macd_dif`,</if>
        <if test="macdDea != null">`macd_dea`,</if>
        <if test="macdMacd != null">`macd_macd`</if>
		)
		values
		(
			#{trancode}, 
			#{time}, 
			#{opening}, 
			#{highest}, 
			#{lowest}, 
			#{closing},
        #{volume},
        <if test="maMa1 != null">#{maMa1},</if>
        <if test="maMa2 != null">#{maMa2},</if>
        <if test="maMa3 != null">#{maMa3},</if>
        <if test="maMa4 != null">#{maMa4},</if>
        <if test="maMa5 != null">#{maMa5},</if>
        <if test="maMa6 != null">#{maMa6},</if>
        <if test="maMa7 != null">#{maMa7},</if>
        <if test="maMa8 != null">#{maMa8},</if>
        #{volVolume}
        <if test="volMavol1 != null">,#{volMavol1},</if>
        <if test="volMavol2 != null">#{volMavol2},</if>
        <if test="macdDif != null">#{macdDif},</if>
        <if test="macdDea != null">#{macdDea},</if>
        <if test="macdMacd != null">#{macdMacd}</if>
		)
	</insert>

