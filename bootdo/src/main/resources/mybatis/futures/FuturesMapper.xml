    </update>

    <delete id="remove">
		delete from tb_futures where id = #{value}
	</delete>

    <delete id="batchRemove">
        delete from tb_futures where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

